# PayByFaceAI Environment Variables
# Copy this file to .env and fill in your actual values

# API Configuration
API_BASE_URL=https://api.dev.facepe.ai
FACE_API_BASE_URL=https://api.dev.facepe.ai

# Stripe Configuration
STRIPE_PUBLISHABLE_KEY=pk_test_your_stripe_publishable_key_here
STRIPE_MERCHANT_DISPLAY_NAME=PayByFaeAi
STRIPE_COUNTRY_CODE=US
STRIPE_CURRENCY=usd

# App Configuration
APP_ENV=development
APP_VERSION=1.0.0

# Feature Flags
ENABLE_FACE_RECOGNITION=true
ENABLE_AUTO_PAY=true
ENABLE_PUSH_NOTIFICATIONS=true

# Debug Settings (development only)
ENABLE_DEBUG_LOGS=true
ENABLE_REDUX_DEVTOOLS=false
